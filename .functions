#!/usr/bin/env bash

function tmp() {
    f=$(mktemp)
    vim "$f"
    echo "$f"
}

function swap() {
    local TMPFILE=tmp.$$
    mv "$1" $TMPFILE && mv "$2" "$1" && mv $TMPFILE "$2"
}

function fghs() {
    git submodule foreach 'echo $sm_path `git rev-parse --short HEAD`'
}

function cf() {
    cheat -l | tail -n +2 | cut -d ' ' -f 1 | fzf | xargs -I{} cheat {}
}
